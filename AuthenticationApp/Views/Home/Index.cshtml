@{
    ViewBag.Title = "Home Page";
}
@using AuthenticationApp.Common

@if (!Request.IsAuthenticated)
{
    @Html.Partial("_Index")
}
else
{

    if (TempData["UserType"] != null && (string) TempData["UserType"] == "Customer")
    {
        @Html.Partial("_CustomerLanding")
    }
    else if (TempData["UserType"] != null && (string) TempData["UserType"] == "Admin")
    {
        @Html.Partial("_VendorLanding")
    }
    else
    {
        @Html.Partial("_AdminLanding")
    }
    
}


